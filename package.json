{
  "name": "freemarker-experiment",
  "version": "2.1.0",
  "main": "index.js",
  "repository": "git@github.com:plumpNation/freemarker-experiment.git",
  "author": "Gavin King <plumpnation@gmail.com>",
  "license": "MIT",
  "scripts": {
    "commit": "git cz",
    "release": "standard-version",
    "build": "webpack --mode production",
    "dev": "webpack --watch --mode development"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-typescript": "7.8.3",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "babel-loader": "8.0.6",
    "commitizen": "4.0.3",
    "css-loader": "3.2.0",
    "cz-conventional-changelog": "3.0.2",
    "husky": "3.0.9",
    "mini-css-extract-plugin": "0.8.0",
    "sass": "1.23.1",
    "sass-loader": "8.0.0",
    "source-map-loader": "0.2.4",
    "standard-version": "7.0.0",
    "style-loader": "1.0.0",
    "ts-loader": "6.2.1",
    "typescript": "3.6.4",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
